Schema::create('roles', function (Blueprint $table) {
            $table->id();
            $table->string('title');
            $table->timestamp('created_at')->useCurrent();
            $table->timestamp('updated_at')->nullable()->useCurrentOnUpdate();
        });
$table->foreignId('role_id')->constrained()->cascadeOnDelete();




